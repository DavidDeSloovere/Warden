@model Warden.Web.Core.Domain.PagedResult<Warden.Web.Core.Dto.OrganizationDto>

<div class="row card-panel">
    <div class="col s12">
        <h4 class="custom-text">Organizations (@Model.Items.Count())</h4>
    </div>
    <div class="col s12">
        <a href="@Url.Action("Create", "Organization")" class="btn waves-effect waves-light custom">Create</a>
    </div>
    <div class="col s12">
        <div class="collection">
            @foreach (var organization in Model.Items)
            {
                <a href="@Url.Action("Details", new {id = @organization.Id})" class="collection-item">
                    <span class="custom-text">@organization.Name</span>
                </a>
            }
        </div>
    </div>
</div>